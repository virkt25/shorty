{
  "name": "shorty",
  "version": "1.0.0",
  "description": "URL Shortening Service",
  "main": "app.js",
  "scripts": {
    "test": "tap test/*.test.js test/*/*.test.js test/*/*/*.test.js && prettier -l \"**/*.js\" \"**/*.md\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -l info -P app.js",
    "lint": "prettier --write \"**/*.js\" \"**/*.md\""
  },
  "author": "Taranveer Virk",
  "license": "MIT",
  "dependencies": {
    "fastify": "^1.9.0",
    "fastify-autoload": "^0.5.0",
    "fastify-cli": "^0.20.0",
    "fastify-mongodb": "^0.9.0",
    "fastify-plugin": "^1.2.0",
    "fastify-static": "^0.14.0",
    "ipware": "^1.0.0",
    "shortid": "^2.2.13"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "tap": "^12.0.0"
  }
}
